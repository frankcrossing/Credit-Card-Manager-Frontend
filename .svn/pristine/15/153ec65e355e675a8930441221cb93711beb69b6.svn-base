import request from '@/utils/request'

export function pagedAgentInfo(query) {
  return request({
    url: '/agent/pagedAgentInfo',
    method: 'post',
    data: query
  })
}

export function saveAgent(agent) {
  return request({
    url: '/agent/save',
    method: 'post',
    data: agent
  })
}
